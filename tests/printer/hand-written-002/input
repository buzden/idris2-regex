interpolate {a=RegExpText ?} $ exists [string "cba", opt (string "abc")] <|> map ? (string "asf" <|> string "bdf") <|> map ? (all [opt (char 'k'), char 'a' <|> char 'b', char 'z']) <|> map ? (all [empty <|> char 'w', opt (empty <|> char 'v'), char 'k']) <|> map ? (all [exists [omega, char 'w'], opt (exists [omega, char 'v']), omega, char 'k']) <|> map ? (all [sym (\k => k == 'a' || k == 'b' || k == 'c'), sym (\k => k /= 'a' && k /= 'b' && k /= 'c'), sym (\k => '0' <= k && k <= '9' || 'a' <= k && k <= 'z' || k == 'D')])
